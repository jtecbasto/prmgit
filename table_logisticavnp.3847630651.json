	"eventHandlers": [
		{
			"eventId": "EVENT_BEFOREADD",
			"pageType": "add",
			"code": "date_default_timezone_set('America/Mexico_City');\n$values['fechaHora'] = date('Y-m-d H:i:s');\n$simcard=$values['SIMEntregado'];\n$idsolicitud=$values['idsolicitud'];\n\n\n// Helpers para armar valores seguros\nfunction q($v){ // cadenas/fechas: 'valor' o NULL\n    return (isset($v) && $v !== '' ? db_prepare_string($v) : \"NULL\");\n}\nfunction qi($v){ // enteros: número o NULL\n    return (isset($v) && $v !== '' ? (int)$v : \"NULL\");\n}\nfunction qb($v){ // booleano/bit: 0/1\n    return (!empty($v) ? 1 : 0);\n}\nfunction qd($v, $scale = 2) { // decimales: número (formateado) o NULL\n    if (!isset($v) || $v === '') return \"NULL\";\n    $n = (float)$v;\n    return number_format($n, $scale, '.', ''); // sin comillas\n}\n\n\n$strSQLExists=\"SELECT\ncompras.noserie\nFROM compras\nLEFT OUTER JOIN ventas v ON compras.noserie = v.esnimei\nLEFT OUTER JOIN movimientos m ON compras.noserie = m.esnimei\nLEFT OUTER JOIN (SELECT t.esnimei,t.tiendadestino,t.`fecharecepcion` FROM traspasosdet t INNER JOIN (SELECT MAX(idtraspasodet) idtraspaso FROM traspasosdet GROUP BY esnimei) t1 ON t.idtraspasodet=t1.idtraspaso) t ON compras.noserie=t.esnimei\nINNER JOIN productos ON compras.codigoproducto = productos.codigoIUSA\nWHERE v.esnimei is null and m.esnimei is null  and (t. esnimei is null or t.fecharecepcion is not null and t.fecharecepcion<>'0000-00-00')\n  and compras.tienda=2 and compras.clasif = 2 and productos.Clasif='Chip' AND compras.noserie='\". q($simcard). \"' LIMIT 1 \";\n \n$rsExists = db_query($strSQLExists,$conn);\n$data = db_fetch_array($rsExists);\n\nif (!$data) {\n    // FALLO: No existe saldo para el número de serie.\n    $message=\"No existe saldo en el inventario para el número de serie\";\n    return false;\n} else {\n    if (!empty($simcard)) {\n       DB::Exec(\"\n            DELETE FROM seriessolicitadas\n            WHERE esnimei=\".q($simcard));        \n    $sql = sprintf(\n        \"INSERT INTO seriessolicitadas (idsolicitud, esnimei) VALUES (%d, %s)\",\n        $idsolicitud,\n        q($simcard)\n    );\n    if (!DB::Exec($sql, $conn)) {\n        // FALLO: No se pudo guardar el equipo.\n        $message = \"No se pudo guardar el equipo en series solicitadas: \".DB::LastError();\n        return false;\n    } \n}\n}\n\n\n\nreturn true;"
		}
	],
