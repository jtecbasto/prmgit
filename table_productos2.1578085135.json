{
	"sql": "select l.idproducto,l.codigoiusa codigo,l.tienda, l.descripcion equipo, l.saldo from (\r\nSELECT p.idproducto, p.codigoIUSA, s.tienda, p.descripcion, s.saldo\r\nFROM (\r\n  SELECT c.codigoproducto,c.tienda, COUNT(*) AS saldo\r\n  FROM compras c\r\n  WHERE c.clasif=2 and c.tienda= :session.Tienda\r\n    AND NOT EXISTS (SELECT 1 FROM ventas v WHERE v.esnimei=c.noserie)\r\n    AND NOT EXISTS (SELECT 1 FROM movimientos m WHERE m.esnimei=c.noserie)\r\n    AND NOT EXISTS (SELECT 1 FROM seriessolicitadas ss WHERE ss.esnimei=c.noserie)\r\n    AND NOT EXISTS (SELECT 1 FROM seriesreservadas  sr WHERE sr.noserie=c.noserie)\r\n  GROUP BY c.codigoproducto\r\n) s\r\nJOIN productos p\r\n  ON p.codigoIUSA = s.codigoproducto\r\n AND p.Clasif = 'Equipo'\r\n\r\n\r\n  UNION ALL\r\n\r\n SELECT p2.idproducto, p2.codigoIUSA, p2.descripcion, 0,0 \r\n  FROM productos p2\r\n  WHERE p2.idproducto = :session.idactual           -- aquí tu prueba\r\n) l\r\n-- opcional: si temes duplicados cuando 1738 sí tiene saldo\r\n-- GROUP BY L.idproducto, L.codigoIUSA, L.descripcion\r\n--         ; -- y si agruparas, usa MAX(L.saldo) saldo\r\n",
