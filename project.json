tant;\r\n}\r\n.table > thead > tr > th a:not(.btn):hover,\r\n/*.table > tbody > tr > td a:not(.btn):hover,*/\r\n.table > tfoot > tr > td a:not(.btn):hover {\r\n  color: var(--accent-hover) !important;\r\n}\r\n\r\n/* Por si la grid no usa .table en alguna página */\r\n.r-grid table a:not(.btn) {\r\n  color: var(--accent) !important;\r\n}\r\n.r-grid table a:not(.btn):hover {\r\n  color: var(--accent-hover) !important;\r\n}\r\n\r\n\r\n\r\n/***********************************************************************/\r\n\r\n",
